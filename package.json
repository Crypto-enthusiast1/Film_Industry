{
   "name": "filmindustry",
   "version": "1.0.0",
   "scripts": {
      "dev": "parcel src/index.html --open",
      "clean": "rimraf dist",
      "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
      "build:js": "parcel build src/index.html --dist-dir dist --public-url ./",
      "copy:video": "copyfiles -u 1 \"src/video/!(videoAbout.*)\" dist",
      "postbuild": "rimraf dist/src",
      "build": "npm run clean && npm run build:css && npm run build:js && npm run copy:video && npm run postbuild"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "devDependencies": {
      "autoprefixer": "^10.4.21",
      "buffer": "^6.0.3",
      "copyfiles": "^2.4.1",
      "parcel": "^2.16.0",
      "postcss": "^8.5.6",
      "postcss-import": "^16.1.1",
      "rimraf": "^6.0.1",
      "tailwindcss": "^3.4.18"
   },
   "dependencies": {
      "firebase": "^12.4.0",
      "firebase-admin": "^13.5.0"
   }
}