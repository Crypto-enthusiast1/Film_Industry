{
   "name": "filmindustry",
   "version": "1.0.0",
   "scripts": {
      "dev": "parcel src/index.html --open",
      "build:js": "parcel build src/index.html --dist-dir dist --public-url ./",
      "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
      "copy:assets": "copyfiles src/img/* dist/img && copyfiles src/video/* dist/video",
      "build": "npm run build:css && npm run build:js && npm run copy:assets"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "devDependencies": {
      "autoprefixer": "^10.4.21",
      "copyfiles": "^2.4.1",
      "parcel": "^2.16.0",
      "postcss": "^8.5.6",
      "postcss-import": "^16.1.1",
      "tailwindcss": "^3.4.18"
   }
}