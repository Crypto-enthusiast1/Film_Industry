{
   "version": 2,
   "builds": [
      {
         "src": "package.json",
         "use": "@vercel/static-build",
         "config": {
            "distDir": "dist"
         }
      }
   ],
   "headers": [
      {
         "source": "/:all*\\.(jpg|jpeg|png|gif|svg|webp)",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      },
      {
         "source": "/:all*\\.(mp4|webm|ogg)",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000"
            }
         ]
      }
   ]
}